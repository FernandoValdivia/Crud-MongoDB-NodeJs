<%- include('partials/_header') %>

<!-- Carousel wrapper -->
<div
  id="carouselDarkVariant"
  class="carousel slide carousel-fade carousel-dark container"
  data-mdb-ride="carousel"
>
  <!-- Indicators -->
  <div class="carousel-indicators">
    <button
      data-mdb-target="#carouselDarkVariant"
      data-mdb-slide-to="0"
      class="active"
      aria-current="true"
      aria-label="Slide 1"
    ></button>
    <button
      data-mdb-target="#carouselDarkVariant"
      data-mdb-slide-to="1"
      aria-label="Slide 1"
    ></button>
    <button
      data-mdb-target="#carouselDarkVariant"
      data-mdb-slide-to="2"
      aria-label="Slide 1"
    ></button>
  </div>

  <!-- Inner -->
  <div class="carousel-inner">
    <!-- Single item -->
    <div class="carousel-item active">
      <img
        src="https://i.pinimg.com/originals/d1/14/d8/d114d889616b64e62a79ab9b31adc6e8.jpg"
        src="http://www.lovethispic.com/uploaded_images/230430-Walnut-Laptop-Stand.jpg"
        class="d-block w-100"
        alt="..."
      />
      <div class="carousel-caption d-none d-md-block">
        <h5>CRUD NodeJs & MongoDB</h5>
        <a href="/index" class="btn btn-dark">Go to CRUD</a>
        <p>Developed by Luis Valdivia</p>
      </div>
    </div>

    <!-- Single item -->
    <div class="carousel-item">
      <img
        src="https://data.whicdn.com/images/187846382/original.jpg"
        class="d-block w-100"
        alt=""
      />
      <div class="carousel-caption d-none d-md-block">
        <h5 >The Topic is Books</h5>
        <a href="/index" class="btn btn-dark">Go to CRUD</a>
        <p >We will perform crud operations to a table of books</p>
      </div>
    </div>

    <!-- Single item -->
    <div class="carousel-item">
      <img
        src="http://www.lovethispic.com/uploaded_images/230430-Walnut-Laptop-Stand.jpg"
        class="d-block w-100"
        alt=""
      />
      <div class="carousel-caption d-none d-md-block">
        <h5 >Yes! Books!</h5>
        <a href="/index" class="btn btn-dark">Go to CRUD</a>
        <p>Any kind of book you can add, delete, watch or update</p>
      </div>
    </div>
  </div>
  <!-- Inner -->

  <!-- Controls -->
  <button
    class="carousel-control-prev"
    type="button"
    data-mdb-target="#carouselDarkVariant"
    data-mdb-slide="prev"
  >
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </button>
  <button
    class="carousel-control-next"
    type="button"
    data-mdb-target="#carouselDarkVariant"
    data-mdb-slide="next"
  >
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </button>
</div>
<!-- Carousel wrapper -->
